package j20220802.awt;

import java.awt.Frame;
import java.awt.TextField;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class AwtText_1 extends Frame implements ActionListener{

	@Override
	public void actionPerformed(ActionEvent e) {
		String str = e.getActionCommand();
		
	}
	public AwtText_1() {
		setSize(300,300);
		setVisible(true);
		addWindowListener(new CommonAwtEvent());
		
		TextField login = new TextField(15);
		login.addActionListener(this);
		TextField passwd = new TextField(15);
		passwd.addActionListener(this);
		
		
	}
	
	
	public static void main(String[] args) {
		AwtText_1 text = new AwtText_1();
	}
	
}
