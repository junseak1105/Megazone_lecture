package questionpool;
import java.util.*;
public class Board_1 {
	int num;
	String title;
	String author;
	String date;
	int hit;
	
	public Board_1(String title,String author,String date,int hit) {
		this.num = num;
		this.title = title;
		this.author = author;
		this.date  = date;
		this.hit = hit;
	}
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("제목 입력");
		Board_1 bd = new Board_1();
	}
}
