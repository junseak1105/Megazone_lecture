package j20220803;

import java.io.File;

public class File_1 {

	
	public static void main(String[] args) {
		
		String path = "/project/share/file1";
		
		File file = new File(path);
		String filearray[] = file.list();
		for (int i = 0; i < filearray.length; i++) {
			System.out.println(filearray[i]);
		}
		
	}
}
