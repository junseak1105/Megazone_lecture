package baek;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int a = in.nextInt();
		int b = in.nextInt();
		float result = 0;
		if(a>b) {
			result = a/b;
		}else {
			result = b/a;
		}
		
		System.out.println(result);
	}

}
