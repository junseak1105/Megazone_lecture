package questionpool;

public class If_Question {

	public static void main(String[] args) {
		int age = 31;
		if(age>=50) {
			System.out.println("50대");
		}
		if(age>=40) {
			System.out.println("40대");
		}
		if (age>=30) {
			System.out.println("30대");
		} else if(age>=20) {
			System.out.println("20대");
		}
	}

}
