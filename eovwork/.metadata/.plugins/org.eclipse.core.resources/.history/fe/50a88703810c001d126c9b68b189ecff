package questionpool;

public class Person {
	public static void main(String[] args) {
		
		String name = "김재훈";
		String address = "해운대구";
		int age = 26;
		age = age +10-20+20-20;;
		
		System.out.println("이름은:"+name);
		System.out.println("주소는:"+address);
		
		System.out.println("나이+10:"+(age+10));
		System.out.println("나이-20:"+(age-20));
		System.out.println("나이+20:"+(age+20));
		System.out.println("나이-20:"+(age-20));
		
	}
}
