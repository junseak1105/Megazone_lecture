package j20220726;

import java.util.Iterator;

public class InnerFor_1 {
	public static void main(String[] args) {
		
		for (int i = 2; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
				if(i==3 || i== 7){
					break;
				}
				System.out.print("\t"+i+"x"+j+"="+i*j+" ");
			}
			if(i!=3 && i!= 7){
				System.out.println();	
			}
			
		}
	}
}
