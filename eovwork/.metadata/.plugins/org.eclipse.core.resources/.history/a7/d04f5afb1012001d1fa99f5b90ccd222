package j20220802.awt;

import java.awt.Button;
import java.awt.Frame;
import java.awt.GridLayout;
import java.awt.TextField;

public class NaverAwt_1 extends Frame {

	public NaverAwt_1() {
			setSize(300,300);
			setVisible(true);
			setLayout(new GridLayout(1,3));
//			Panel bodypanel = new Panel();
			
			addWindowListener(new CommonAwtEvent());
			
			TextField login = new TextField(15);
			login.addActionListener(new CommonAwtEvent());
			TextField passwd = new TextField(15);
			passwd.addActionListener(new CommonAwtEvent());
			
			Button btn = new Button("input");
//			bodypanel.add(login);
//			bodypanel.add(passwd);
			
			add(login);
			add(passwd);
			add(btn);
			
		}

	public static void main(String[] args) {
		NaverAwt_1 text = new NaverAwt_1();
	}

}
