package baek;

import java.util.Iterator;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		
		for (int i = 1; i <= num; i++) {
			for(int j=num-i;j>0;j--) {
				System.out.print("");
			}
			for(int k = 1;k<=i;k++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}