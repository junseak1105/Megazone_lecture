public class LoginFrame extends Frame{


	public LoginFrame() {
		setSize(500,500);
		setVisible(true);
		addWindowListener(new commonWindow());
		
		Button btn1 = new Button("to Main");
//		btn1.addActionListener(new Actions());
		add(btn1);
		
	}
	
	public static void main(String[] args) {
		LoginFrame mf = new LoginFrame();
		
	}
}
